    #<You can see your character and move during cutscenes.>
   
    set SeqAct_ToggleCinematicMode bDisableMovement False

    set SeqAct_ToggleCinematicMode bHidePlayer False

    set SeqAct_ToggleCinematicMode bDisableInput False

    set SeqAct_ToggleCinematicMode bDisableTurning False
    
    #</You can see your character and move during cutscenes.>
